---
import Facebook from '../atoms/Facebook.astro';
import Instagram from '../atoms/Instagram.astro';

import { SanityClient } from '../lib/sanity';

const some = await SanityClient.fetch('*[ _type == "settingsSome" ]').then(
  (res) => res[0]
);
---

<Facebook fbUrl={some.facebook} />
<Instagram igUrl={some.instagram} />
